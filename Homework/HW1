    "# task 8\n",
    "import datetime\n",
    "tweets = []\n",
    "for line in open(\"CrimeReport.txt\",\"r\").readlines():\n",
    "    tweet = json.loads(line)\n",
    "    tweets.append(tweet)\n",
    "sortedtweets = sorted(tweets, key = lambda item :datetime.datetime.strptime(item['created_at'], '%a %b %d %H:%M:%S +0000 %Y'))\n",
    "f = open('output.txt', 'w')\n",
    "for tweet in sortedtweets:\n",
    "    print(tweet['created_at'])\n",
    "    f.write(json.dumps(tweet) +'\\n' )\n",
    "f.close()"
   ]
