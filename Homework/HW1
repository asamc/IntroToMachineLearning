   "source": [
    "# task 9\n",
    "# output log is the grouped list of sorted tweets, which are also sent to files in task9-output\n",
    "import datetime\n",
    "tweets = []\n",
    "for line in open(\"CrimeReport.txt\",\"r\").readlines():\n",
    "    tweet = json.loads(line)\n",
    "    tweets.append(tweet)\n",
    "sortedtweets = sorted(tweets, key = lambda item :datetime.datetime.strptime(item['created_at'], '%a %b %d %H:%M:%S +0000 %Y'))\n",
    "\n",
    "# take the sorted list and group by hours\n",
    "hourList = []\n",
    "\n",
    "for tweet in sortedtweets:\n",
    "    tweetIsGrouped = False\n",
    "    for hourGroup in hourList:\n",
    "        if hourGroup[0].get('created_at')[:13] in tweet['created_at']: # this part checks if a tweet that was \n",
    "            hourGroup.append(tweet)                                    # posted in the same hour has already been added\n",
    "            tweetIsGrouped = True                                      # and if so, adds it there\n",
    "    if not tweetIsGrouped:                                             # otherwise, it makes a new group for tweets of the new hour\n",
    "        newHourGroup = [tweet]\n",
    "        hourList.append(newHourGroup)\n",
    "\n",
    "for hourGroup in hourList: #for each group, creates a Mon-Day-Year-Hour.txt file and dumps the jsons into them\n",
    "    f = open('task9-output/'+hourGroup[0].get('created_at')[4:7]+'-'+hourGroup[0].get('created_at')[8:10]+'-'+hourGroup[0].get('created_at')[26:]+'-'+hourGroup[0].get('created_at')[11:13], 'w')\n",
    "    for tweet in hourGroup:\n",
    "        print(tweet['created_at'])\n",
    "        f.write(json.dumps(tweet)+'\\n')\n",
    "    print('')\n"
   ]
  },
